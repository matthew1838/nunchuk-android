<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/non_subscriber_navigation"
    app:startDestination="@id/nonSubscriberIntroFragment">

    <fragment
        android:id="@+id/nonSubscriberIntroFragment"
        android:name="com.nunchuk.android.main.nonsubscriber.intro.NonSubscriberIntroFragment"
        android:label="NonSubscriberIntroFragment">

        <argument
            android:name="banner_id"
            app:argType="string" />
        <action
            android:id="@+id/action_nonSubscriberIntroFragment_to_tryAssistedWalletFragment"
            app:destination="@id/tryAssistedWalletFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/tryAssistedWalletFragment"
        android:name="com.nunchuk.android.main.nonsubscriber.tryassisted.TryAssistedWalletFragment"
        android:label="TryAssistedWalletFragment" />
</navigation>